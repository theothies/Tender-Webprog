{
  "name": "tender",
  "version": "1.0.1",
  "author": "Robin Boehringer, Enes Oezdogan, Theodor Thiessen",
  "description": "Tender Single Page Web App",
  "keywords": [
    "angular",
    "angular 8",
    "material",
    "nosql",
    "oauth",
    "starter-template",
    "ngx"
  ],
  "repository": "https://github.com/Enes2802/tender",
  "license": "",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "reset": "rm -rf ./node_modules ./package-lock.json && npm install",
    "dist": "ng build",
    "prod": "ng build --prod",
    "prod:hashless": "ng build --prod --output-hashing none",
    "prod:src": "ng build --prod --sourcemaps",
    "compodoc:g": "compodoc -p src/tsconfig.app.json",
    "compodoc:s": "compodoc -p src/tsconfig.app.json --serve",
    "compodoc:w": "compodoc -p src/tsconfig.app.json --serve --watch",
    "tscov": "tscov -m 90",
    "tscov:d": "tscov --details",
    "link-upstream": "git remote add upstream https://github.com/jeroenouw/AngularMaterialFirebase",
    "sync": "git fetch && git checkout master && git merge upstream/master && git push"
  },
  "dependencies": {
    "@angular/animations": "^8.2.13",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.13",
    "@angular/compiler": "^8.2.13",
    "@angular/core": "^8.2.13",
    "@angular/fire": "^5.2.1",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "^8.2.13",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.13",
    "@angular/platform-browser-dynamic": "^8.2.13",
    "@angular/platform-server": "^8.2.13",
    "@angular/router": "^8.2.13",
    "core-js": "^3.4.0",
    "firebase": "^7.3.0",
    "hammerjs": "^2.0.8",
    "ngx-auth-firebaseui": "^3.4.2",
    "rxjs": "^6.5.3",
    "rxjs-compat": "^6.5.3",
    "ts-helpers": "*",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.18",
    "@angular/cli": "^8.3.18",
    "@angular/compiler-cli": "^8.2.13",
    "@angular/language-service": "^8.2.13",
    "@compodoc/compodoc": "^1.1.11",
    "@liftr/tscov": "^1.4.3",
    "@types/jasmine": "^3.4.6",
    "@types/node": "^12.12.7",
    "codelyzer": "^5.2.0",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^6.3.14",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "^5.4.2",
    "rxjs-tslint": "^0.1.7",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.1",
    "webpack": "^4.41.2"
  },
  "peerDependencies": {
    "typescript": "3.7.2"
  }
}
